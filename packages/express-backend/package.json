{
  "name": "@tendec/express-backend",
  "version": "1.0.0",
  "license": "MIT",
  "compilerOptions": {
    "target": "es5",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noFallthroughCasesInSwitch": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "jsx": "react-jsx"
  },
  "include": [
    "src"
  ],
  "scripts": {
    "start": "ts-node src/index.ts",
    "build": "tsc",
    "serve": " node dist/index.js"
  },
  "dependencies": {
    "@tendec/shared-types": "*",
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.0",
    "csv-parse": "^5.5.2",
    "express": "^4.18.2",
    "ts-node": "^10.9.1",
    "typescript": "^5.3.2"
  }
}
